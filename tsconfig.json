{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "allowJs": true,
    "checkJs": false,
    "sourceMap": false,
    "strict": false,
    "noEmit": false,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "@nuxt/types",
      "@nuxt/typescript-build",
      "@types/node"
    ]
  },
  "include": [
    "src/ts/**/*",
  ]
}
